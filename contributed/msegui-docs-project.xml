<?xml version="1.0" encoding="UTF-8"?>
<docproject>
  <packages>
    <!-- Multiple packages can be entered. 
         If only one is specified, it is selected. 
         "name" is a mandatory attribute
         a "units" tag is required, and a "descriptions" tag as well
    -->
    <package name="msegui" output="msegui.ipf" contentfile="fpgui.cnt">
<!--
    <package name="msegui" output="msegui.ipf" contentfile="fpgui.cnt">
    <package name="msegui" output="html/" contentfile="msegui.cnt">
-->
      <!-- All input files, one "unit" tag per unit -->
      <units>
        <!-- "file" is a mandatory attribute, "options" is not mandatory -->
        <!-- <unit file="../src/corelib/fpg_base.pas" options="-Fi../src -Fu../src/corelib/x11/ -Fi../src/corelib/x11/ -Fu../src/gui/ -Fu../src/corelib/"/>  -->

        <!-- undocumented units -->
        <unit file="../lib/common/kernel/msetypes.pas" options="-dmse_with_ifi -Fi../lib/common/kernel -Fu../lib/common/kernel"/>

        <unit file="../lib/common/kernel/msearrayprops.pas" options="-Fu../lib/common/kernel"/>
        <unit file="../lib/common/kernel/msearrayutils.pas" options="-Fu../lib/common/kernel"/>
        <unit file="../lib/common/kernel/msebintree.pas" options="-Fu../lib/common/kernel"/>
        <unit file="../lib/common/kernel/msebits.pas" options="-Fu../lib/common/kernel"/>
        <unit file="../lib/common/kernel/msebucketlist.pas" options="-Fu../lib/common/kernel"/>
        <unit file="../lib/common/kernel/mseclasses.pas" options="-Fu../lib/common/kernel"/>
        <unit file="../lib/common/kernel/mseconsts.pas" options="-Fu../lib/common/kernel"/>
<!--        
        <unit file="../lib/common/kernel/mseactions.pas" options="-Fu../lib/common/kernel"/>
